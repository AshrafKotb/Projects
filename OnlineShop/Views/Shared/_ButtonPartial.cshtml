@model int
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<td style="width:150px">
    <div class="btn-group">
        <a asp-action="Edit"  class=" btn btn-primary "data-toggle="tooltip"  title="Edit" asp-route-id="@Model">
        <i class="fa fa-edit" ></i> </a>
        <a asp-action="Details"  class="btn btn-success"data-toggle="tooltip"  title="Details" asp-route-id="@Model">
            <i class="fa fa-list-alt"></i>
        </a>
        <a asp-action="Delete"  class="btn btn-danger"data-toggle="tooltip"  title="Delete" asp-route-id="@Model">
            <i class="fa fa-trash-alt"></i>
        </a>
    </div>
</td>